package primeiroProjeto;

import java.util.Scanner;

public class Teste {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		int num5 = 0;
		for(int ex5 = 1; ex5 <= 50; ex5 ++) {
			if (ex5%3 == 0) {
				num5 += 1;
			}
		}
		System.out.println(num5);
		
	}

}
